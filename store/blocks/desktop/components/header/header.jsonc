{
  "header-row#header__top--desktop": {
    "children": [
      "rich-text#contacto",
      "header-spacer",
      "rich-text#falabella"
    ],
    "props": {
      "blockClass": "header_top"
    }
  },
  "rich-text#contacto": {
    "props": {
      "text": "Venta Telefónica 01 8000 12 7373 | Servicio al cliente 320 88 999 33 | Tiendas | Precios bajos siempre | Mundo CMR",
      "font": "t-heading-1"
    }
  },
  "rich-text#falabella": {
    "props": {
      "text": "FALABELLA | BANCO | SEGUROS | SEGUIMIENTO DE COMPRA",
      "font": "t-heading-1"
    }
  },
  "header-row#header__middle--desktop": {
    "children": [
      "logo#header__top--logo",
      "search-bar#header__home",
      "minicart.v2#desktop",
      "login",
      "modal-trigger#header__middle--login"
    ],
    "props": {
      "blockClass": "header__top"
    }
  },
  "logo#header__top--logo": {
    "title": "logo",
    "props": {
      "url": "assets/img/homecenter.jpg",
      "width": 200,
      "href": "/"
    }
  },
  "modal-trigger#header__middle--login": {
    "children": [
      "rich-text#header__middle--login",
      "modal-layout#header__middle--login"
    ]
  },
  "login": {
    "title": "Login",
    "props": {
      "showIconProfile": true,
      "hideIconLabel": true
    }
  },
  "rich-text#header__middle--login": {
    "props": {
      "text": "Mi cuenta"
    }
  },
  "modal-layout#header__middle--login": {
    "children": [
      "login-content"
    ]
  },
  "search-bar#header__home": {
    "blocks": [
      "autocomplete-result-list.v2"
    ],
    "props": {
      "placeholder": "¿Qué estás buscando?",
      "openAutocompleteOnFocus": true
    }
  },
  "minicart.v2#desktop": {
    "props": {
      "MinicartIcon": "image#icon-minicart",
      "itemCountMode": "total",
      "quantityDisplay": "always"
    }
  },
  "icon#minicart": {
    "props": {
      "id": "hpa-calendar",
      "size": 40
    }
  },
  "image#icon-minicart": {
    "props": {
      "src": "assets/img/icons/icon-cart.png",
      "width": 30
    }
  }
}
